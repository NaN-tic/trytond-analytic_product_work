<data>
    <xpath expr="/form/field[@name='name']" position="after">
        <label string="name" id="rec_name"/>
        <field name="rec_name"/>
    </xpath>
    <xpath expr="/form/field[@name='company']" position="after">
        <newline/>
        <label name="product"/>
        <field name="product"/>
        <label name="account"/>
        <field name="account"/>
    </xpath>
</data>
